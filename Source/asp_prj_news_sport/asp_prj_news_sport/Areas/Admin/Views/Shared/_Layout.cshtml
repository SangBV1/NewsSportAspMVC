<!DOCTYPE html>
<head>
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Colored Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- bootstrap-css -->
    <link href="/assets/admin/components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="/assets/admin/components/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <!-- //bootstrap-css -->
    <!-- Custom CSS -->
    <link href="~/assets/admin/css/style.css" rel='stylesheet' type='text/css' />
    <!--Table-->
    <link href="~/assets/admin/css/table-style.css" rel="stylesheet" />
    <link href="~/assets/admin/css/basictable.css" rel="stylesheet" />
    <!-- font CSS -->
    <link href='//fonts.googleapis.com/css?family=Roboto:400,100,100italic,300,300italic,400italic,500,500italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>
    <!-- font-awesome icons -->
    <link href="~/assets/admin/components/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/assets/admin/css/font.css" type="text/css" />
    <link href="~/assets/admin/css/font-awesome.css" rel="stylesheet" />
    <!-- //font-awesome icons -->
    <script src="~/assets/admin/js/jquery2.0.3.min.js"></script>
    <script src="~/assets/admin/js/modernizr.js"></script>
    <script src="~/assets/admin/js/jquery.cookie.js"></script>
    <script src="~/assets/admin/js/screenfull.js"></script>
    <script>
        $(function () {
            $('#table').basictable();

			$('#supported').text('Supported/allowed: ' + !!screenfull.enabled);

			if (!screenfull.enabled) {
				return false;
			}
			$('#toggle').click(function () {
				screenfull.toggle($('#container')[0]);
			});
		});
    </script>
    <!-- charts -->
    <script src="~/assets/admin/js/raphael-min.js"></script>
    <script src="~/assets/admin/js/morris.js"></script>
    <link rel="stylesheet" href="~/assets/admin/css/morris.css">
    <!-- //charts -->
    <!--skycons-icons-->
    <script src="~/assets/admin/js/skycons.js"></script>
    <!--//skycons-icons-->
</head>
<body class="dashboard-page">
    @RenderPage("nav_menu_left.cshtml")
    <section class="wrapper scrollable">
        <nav class="user-menu">
            <a href="javascript:;" class="main-menu-access">
                <i class="icon-proton-logo"></i>
                <i class="icon-reorder"></i>
            </a>
        </nav>
        @RenderPage("title_bar.cshtml")

        <div class="main-grid">
            <div class="agile-grids">
             
                @if (TempData["AlertMessage"] != null)
                {
                    <div id="AlertBox" class="alert @TempData["AlertType"] hide">
                        @TempData["AlertMessage"]
                    </div>
                }
                
                @RenderBody()
            </div>
        </div>

        <div class="footer">
            <p>© 2017 TorWD</a></p>
        </div>
    </section>
    <script src="~/assets/admin/js/jquery.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/assets/admin/components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/assets/admin/js/proton.js"></script>
    <script src="~/assets/admin/js/sb-admin.js"></script>
    <script src="~/assets/admin/js/jquery.basictable.min.js"></script>
    <script src="~/assets/admin/js/plugins/ckfinder/ckfinder.js"></script>
    <script src="~/assets/admin/js/plugins/ckeditor/ckeditor.js"></script>
    @RenderSection("jsFooter", required: false)
</body>
</html>
